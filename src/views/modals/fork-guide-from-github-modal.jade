.modal-content
  .modal-header
    p.modal-title.text-center.title-font {{ title }}
  .modal-body
    .container-fluid
      .row
        .col-md-12
          slug

  .modal-footer
    .row
      .col-md-12.pull-right
        button.btn.btn-default(type='button', ng-click='no()', data-dismiss='modal') {{ 'cancel' | translate }}
        button.btn.btn-success(type='button', spin-on-click='yes()', ng-disabled='!CurrentItem.getOrganization()') {{ 'import' | translate }}
